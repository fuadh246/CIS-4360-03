//study(title="SMA Difference", shorttitle="SMAD")
study("CIS4360_SMA", overlay=true)
//sym = input(title="Symbol", type=symbol, defval="BINANCE:BTCUSDT", confirm=true)
src = input(close, title="Source")

price = security(tickerid, period, src)
price_sma9 = sma(price, 9)
price_sma20 = sma(price, 20)
price_sma50 = sma(price, 50)
price_sma100 = sma(price, 100)
price_ema = ema(price, 20)
plot(series=price_sma9, style=line, color=orange, linewidth=2, transp=0)
plot(series=price_sma20, style=line, color=blue, linewidth=2, transp=0)
plot(series=price_sma50, style=line, color=green, linewidth=2, transp=0)
plot(series=price_sma100, style=line, color=purple, linewidth=2, transp=0)
plot(series=price_ema, style=line, color=maroon, linewidth=2, transp=0)
